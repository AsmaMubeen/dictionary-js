{
  "name": "dictionary-cli-tool",
  "description": "Command line dictionary tool",
  "version": "1.0.2",
  "author": "asma mubeen",
  "bin": {
    "dict": "./bin/run"
  },
  "bugs": "https://github.com/AsmaMubeen/dictionary-js/issues",
  "dependencies": {
    "@oclif/command": "^1.5.19",
    "@oclif/config": "^1.13.3",
    "@oclif/plugin-help": "^2.2.1",
    "@types/axios": "^0.14.0",
    "axios": "^0.19.0",
    "chalk": "^2.4.2",
    "cli-ux": "^5.3.3",
    "inquirer": "^7.0.0"
  },
  "devDependencies": {
    "@oclif/dev-cli": "^1.22.2",
    "eslint": "^5.16.0",
    "eslint-config-oclif": "^3.1.0",
    "globby": "^10.0.1",
    "rimraf": "^3.0.0"
  },
  "engines": {
    "node": ">=8.0.0"
  },
  "files": [
    "/bin",
    "/npm-shrinkwrap.json",
    "/oclif.manifest.json",
    "/src"
  ],
  "homepage": "https://github.com/AsmaMubeen/dictionary-js",
  "keywords": [
    "oclif"
  ],
  "license": "MIT",
  "main": "src/index.js",
  "oclif": {
    "commands": "./src/commands",
    "bin": "dict",
    "plugins": [
      "@oclif/plugin-help"
    ]
  },
  "repository": "https://github.com/AsmaMubeen/dictionary-js",
  "scripts": {
    "postpack": "rimraf oclif.manifest.json",
    "posttest": "eslint .",
    "prepack": "oclif-dev manifest && oclif-dev readme",
    "test": "echo NO TESTS",
    "version": "oclif-dev readme && git add README.md",
    "postinstall": "npm link"
  }
}
